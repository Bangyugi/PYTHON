    "from math import* \n",
    "\n",
    "def solve(n,k):\n",
    "    res = 1\n",
    "    for i in range(1,k+1):\n",
    "        res*=(n-i+1)\n",
    "        res//=i\n",
    "    return res\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    n,k = map(int,input().split())\n",
    "    print (comb(n,k))\n",
    "    print (solve(n,k))"
   ]
