    "from math import* \n",
    "\n",
    "def square(n):\n",
    "    can = isqrt(n)\n",
    "    return can **2 == n\n",
    "\n",
    "def cube(n):\n",
    "    can = int(cbrt(n))\n",
    "    return can**3 == n\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    n = int(input())\n",
    "    if cube(n):\n",
    "        print(\"YES\")\n",
    "    else:\n",
